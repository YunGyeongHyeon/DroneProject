<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" minHeight="40.0" minWidth="500.0" style="-fx-border-width: 0 0 1 0; -fx-border-color: black;" styleClass="reportitem" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label id="lblNo" alignment="CENTER" prefWidth="100.0" text="사건번호" textFill="WHITE" />
      <Label id="lblLat" alignment="CENTER" maxWidth="1.7976931348623157E308" text="위도" textFill="WHITE" HBox.hgrow="ALWAYS" />
      <Label id="lblLng" alignment="CENTER" maxWidth="1.7976931348623157E308" text="경도" textFill="WHITE" HBox.hgrow="ALWAYS" />
      <Label id="lblTime" alignment="CENTER" maxWidth="1.7976931348623157E308" text="시간" textFill="WHITE" HBox.hgrow="ALWAYS" />
      <Button id="btnMap" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="지도표시" textFill="WHITE" />
      <Button id="btnComplete" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="완료처리" textFill="WHITE" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
